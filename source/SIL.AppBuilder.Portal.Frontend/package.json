  "scripts": {
    "remove-semantic-types": "rm -f node_modules/semantic-ui-react/index.d.ts",
    "ts-node": "./node_modules/.bin/ts-node",
    "tsc": "./node_modules/.bin/tsc",
    "karma": "./node_modules/.bin/karma",
    "webpack": "./node_modules/.bin/webpack-cli",
    "polly": "./node_modules/.bin/polly",
    "webpack-dev": "./node_modules/.bin/webpack-dev-server",
    "tslint": "./node_modules/.bin/tslint -c ./tslint.json --project ./tsconfig.json",
    "start:dev": "yarn remove-semantic-types && REVISION=$(git rev-parse HEAD) BUILD_DATE=$(date) yarn webpack-dev",
    "webpack:build": "BUILD_DATE=$(date) NODE_ENV=production yarn webpack --env production",
    "webpack:build:analyze": "./scripts/analyze.sh",
    "webpack:analyze": "./node_modules/.bin/webpack-bundle-analyzer dist/stats.json",
    "analyze": "yarn webpack:build:analyze && yarn webpack:analyze",
    "karma:start": "yarn remove-semantic-types && yarn karma start ./tests/karma.conf.js --fail-on-empty-test-suite",
    "karma:debug": "yarn karma start ./tests/karma.conf.js --fail-on-empty-test-suite --browsers ChromeDebug",
    "test:watch": "yarn karma:start",
    "test:watch:headless": "CI=true yarn karma:start",
    "test:watch:detached": "DETACHED=true yarn karma:start",
    "test:ci": "CI=true yarn karma:start --single-run",
    "lint": "yarn lint:js",
    "lint:js": "yarn eslint . --ext .js --ext .ts --ext .jsx --ext .tsx",
    "lint:js:fix": "yarn lint --fix",
    "tslint-check": "./node_modules/.bin/tslint-config-prettier-check ./tslint.json",
    "typecheck": "yarn tsc --noEmit"
  },
