[
  {
    "key": "workflowbar",
    "data-buildertype": "workflowbar",
    "events": {
      "onCommandClick": {
        "active": true,
        "actions": [
          "workflowExecuteCommand"
        ]
      },
      "onSetStateClick": {
        "active": true,
        "actions": [
          "workflowSetState"
        ]
      }
    },
    "blockSetState": true
  },
  {
    "key": "form_1",
    "data-buildertype": "form",
    "children": [
      {
        "key": "input_repository_location",
        "data-buildertype": "input",
        "label": "Repository Location",
        "fluid": true,
        "onChangeTimeout": 200,
        "readOnly": true
      },
      {
        "key": "header_1",
        "data-buildertype": "header",
        "content": "Complete the following before clicking on the Continue button:",
        "size": "medium"
      },
      {
        "key": "checkbox_1",
        "data-buildertype": "checkbox",
        "label": "I have pasted the above \"repository location\" into the \"Publishing\" page.",
        "events": {},
        "other-required": true
      },
      {
        "key": "checkbox_2",
        "data-buildertype": "checkbox",
        "label": "I have clicked \"Send\" in the \"Publishing\" page and successfully uploaded by app data to the app publishing repository."
      }
    ]
  }
]