{
  "labels": [
    "dependency upgrade"
  ],
  "masterIssue": true,
  "extends": [
    "config:base"
  ],
  "automerge": false,
  "enabled": true,
  "ignorePaths": [
  ],
  "packageRules": [
    {
      "packagePatterns": ["^@types"],
      "groupName": "TypeScript type definitions"
    },
    {
        "paths": ["package.json"],
        "rangeStrategy": "replace",
        "enabled": true,
        "automerge": true
    },
    {
        "packagePatterns": ["Dockerfile"],
        "enabled": false
    }
  ]
}
